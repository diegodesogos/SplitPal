{
  "name": "expense-sharing-app",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "cross-env NODE_ENV=development NODE_OPTIONS=\"-r dotenv/config\" tsx watch server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/server",
    "start": "cross-env NODE_ENV=production NODE_OPTIONS=\"-r dotenv/config\" node dist/server/index.js",
    "preview": "npm run build && npm run start",
    "check": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "test:server": "vitest --config ./server/vitest.config.ts --run",
    "test:server:watch": "vitest --config ./server/vitest.config.ts",
    "test:client": "vitest --config ./client/vitest.config.ts --run",
    "test:client:watch": "vitest --config ./client/vitest.config.ts",
    "test:e2e": "playwright test"
  },
