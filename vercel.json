{
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.)",
      "dest": "server/index.ts"
    },
    {
      "src": "/(.)",
      "dest": "/dist/client/index.html"
    }
  ]
}
