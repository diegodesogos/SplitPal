{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/client/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/server/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "dist/client/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/client/index.html"
    }
  ]
}
